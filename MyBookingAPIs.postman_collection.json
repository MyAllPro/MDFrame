		{
			"name": "4-GetBooking by id",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.sendRequest({\r",
							"    method : \"POST\",\r",
							"    url:pm.environment.get(\"baseURL\")+'/auth',\r",
							"    header:{\r",
							"        'content-type':'application/json'\r",
							"    },\r",
							"    body:{\r",
							"        mode:\"raw\",\r",
							"        raw:JSON.stringify\r",
							"        ({\r",
							"            \"firstname\":pm.environment.get(\"firstname\") ,      \r",
							"             \"lastname\":pm.environment.get(\"lastname\"),\r",
							"             \"totalprice\":pm.environment.get(\"totalprice\")\r",
							"\r",
							"        })\r",
							"\r",
							"    \r",
							"}, function(error,response)\r",
							"{\r",
							"    if(error)\r",
							"    {\r",
							"      console.log(error);  \r",
							"    }\r",
							"    else{\r",
							"                const responseBody=JSON.parse(response.text());\r",
							"       // const response = pm.response.text();\r",
							"        const bookingid = responseBody.bookingid;\r",
							"        console.log(\"new booking id ==> \" +bookingid);\r",
							"        pm.variables.set(\"new_BookID\", bookingid);\r",
							"\r",
							"    }\r",
							"}\r",
							"\r",
							"})"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
